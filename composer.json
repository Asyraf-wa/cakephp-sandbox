{
	"name": "dereuromark/cakephp-sandbox",
	"description": "A CakePHP Sandbox App",
	"minimum-stability": "dev",
	"homepage" : "http://sandbox.dereuromark.de/",
	"authors": [
		{
			"name": "Mark Scherer",
			"email": "euromark@web.de",
			"homepage": "http://www.dereuromark.de",
			"role" : "Project Manager"
		}
	],
	"keywords" : [
		"CakePHP",
		"Plugin",
		"Sandbox"
	],
	"license": "MIT",
	"require": {
		"cakephp/cakephp": "3.0.*-dev",
		"mobiledetect/mobiledetectlib": "2.*",
		"dereuromark/cakephp-tinyauth": "dev-master",
		"dereuromark/cakephp-geo": "dev-master",
		"dereuromark/cakephp-tools": "dev-cake3",
		"dereuromark/cakephp-setup": "dev-cake3",
		"friendsofcake/authenticate": "dev-cake3",
		"CakeDC/search" : "3.0.*-dev",
		"cakephp/migrations": "dev-master",

		"markstory/asset_compress" : "3.0.*-dev",
		"dereuromark/cakephp-data" : "dev-cake3",
		"dereuromark/cakephp-feed" : "dev-master",
		"dereuromark/cakephp-ajax" : "dev-master",
		"friendsofcake/cakepdf": "3.0.*-dev",
		"markstory/geshi": "3.0.*-dev",
		"dereuromark/cakephp-shim": "3.0.*-dev"
	},
	"require-dev": {
		"psy/psysh": "@stable",
		"cakephp/debug_kit": "3.0.*-dev",
		"cakephp/bake": "dev-master",
		"dereuromark/cakephp-codesniffer": "3.0.*-dev",
		"dereuromark/cakephp-upgrade": "dev-cake3"
	},
	"minimum-stability" : "dev",
	"prefer-stable": false,
	"autoload": {
		"psr-4": {
			"App\\": "src",
			"Sandbox\\": "src/Plugin/Sandbox/src",
			"AuthSandbox\\": "src/Plugin/AuthSandbox/src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Test\\": "tests",
			"Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
		}
	},
	"repositories" : [{
			"type" : "vcs",
			"url" : "https://github.com/dereuromark/search"
		}
	],
	"support": {
		"source": "https://github.com/dereuromark/cakephp-sandbox"
	}
}
